{% extends 'layout.html.twig' %}
 

{% block content %}
    <div id="main" role="main" class="container">
    <h1>Login</h1>
        {% if error %}
            <div class="alert alert-error">
                {{ error }}
            </div>
        {% endif %}

        <form action="" method="post" novalidate {{ form_enctype(form) }} class="form-vertical">
            {{ form_widget(form) }}
        </form>
    </div>
{% endblock %}